{
  "name": "game-of-life",
  "version": "1.0.0",
  "description": "A web implementation of conway's game of life",
  "main": "index.js",
  "scripts": {
    "lint": "eslint src/**/*.{js,vue} --fix",
    "prettier": "prettier --write **/*.{md,html,js,vue}",
    "dev": "NODE_ENV=development webpack-dev-server --open",
    "test": "jest --coverage",
    "build": "webpack"
  },
  "keywords": [
    "gameoflife"
  ],
  "author": "Swann Castel",
  "license": "ISC",
  "devDependencies": {
    "@babel/preset-env": "^7.9.5",
    "babel-jest": "^25.4.0",
    "clean-webpack-plugin": "^1.0.1",
    "css-loader": "^3.5.3",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-vue": "^7.0.0-alpha.1",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^4.2.5",
    "jest": "^25.4.0",
    "lint-staged": "^10.1.7",
    "vue-loader": "^15.9.1",
    "vue-template-compiler": "^2.6.8",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.2.3",
    "webpack-dev-server": "^3.10.3"
  },
  "dependencies": {
    "vue": "^2.6.8"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,vue}": "eslint --fix",
    "*.{md,html,js,vue}": "prettier --write",
    "*.js": [
      "jest --findRelatedTests"
    ]
  }
}
